<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.itaz.servlet.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.itaz.servlet.Register</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>openservice</servlet-name>
    <servlet-class>com.itaz.servlet.OpenService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>openservice</servlet-name>
    <url-pattern>/openservice</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>closeservice</servlet-name>
    <servlet-class>com.itaz.servlet.CloseService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>closeservice</servlet-name>
    <url-pattern>/closeservice</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.itaz.Linster.TaskLinster</listener-class>
  </listener>
  <filter>
    <filter-name>charset</filter-name>
    <filter-class>com.itaz.filter.CharSet</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>charset</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>main</filter-name>
    <filter-class>com.itaz.filter.MainSent</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>main</filter-name>
    <url-pattern>/main.jsp</url-pattern>
  </filter-mapping>
</web-app>
